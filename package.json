    "conventional-changelog-cli": "^1.3.5",
    "conventional-gitlab-releaser": "^2.1.10",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "should": "^13.1.3"
